{
  "name": "@ensolid/visx",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./Group": {
      "import": "./dist/Group/Group.js",
      "types": "./dist/Group/Group.d.ts"
    },
    "./Bar": {
      "import": "./dist/Shape/Bar.js",
      "types": "./dist/Shape/Bar.d.ts"
    },
    "./LinePath": {
      "import": "./dist/Shape/LinePath.js",
      "types": "./dist/Shape/LinePath.d.ts"
    },
    "./Area": {
      "import": "./dist/Shape/Area.js",
      "types": "./dist/Shape/Area.d.ts"
    },
    "./Arc": {
      "import": "./dist/Shape/Arc.js",
      "types": "./dist/Shape/Arc.d.ts"
    },
    "./Pie": {
      "import": "./dist/Shape/Pie.js",
      "types": "./dist/Shape/Pie.d.ts"
    },
    "./AxisBottom": {
      "import": "./dist/Axis/AxisBottom.js",
      "types": "./dist/Axis/AxisBottom.d.ts"
    },
    "./AxisLeft": {
      "import": "./dist/Axis/AxisLeft.js",
      "types": "./dist/Axis/AxisLeft.d.ts"
    },
    "./GridRows": {
      "import": "./dist/Grid/GridRows.js",
      "types": "./dist/Grid/GridRows.d.ts"
    },
    "./GridColumns": {
      "import": "./dist/Grid/GridColumns.js",
      "types": "./dist/Grid/GridColumns.d.ts"
    },
    "./Text": {
      "import": "./dist/Text/Text.js",
      "types": "./dist/Text/Text.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "d3-array": "^3.2.4",
    "d3-format": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "d3-time-format": "^4.1.0"
  },
  "devDependencies": {
    "@types/d3-array": "^3.2.1",
    "@types/d3-format": "^3.0.4",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-shape": "^3.1.6",
    "@types/d3-time-format": "^4.0.3",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-solid": "^2.11.8"
  },
  "peerDependencies": {
    "solid-js": "^1.9.9"
  }
}
