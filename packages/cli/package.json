{
  "name": "@ensolid/cli",
  "version": "0.0.1",
  "description": "Ensolid CLI for managing components",
  "type": "module",
  "bin": {
    "ensolid": "./dist/index.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean --dts",
    "dev": "tsup src/index.ts --format esm --watch --clean",
    "lint": "eslint src",
    "publish": "pnpm build && pnpm publish"
  },
  "keywords": [
    "solidjs",
    "shadcn",
    "cli",
    "ui"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
