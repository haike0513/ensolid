{
  "name": "@ensolid/cli",
  "version": "0.0.1",
  "description": "CLI for adding beautiful, accessible SolidJS components to your projects. Inspired by shadcn/ui.",
  "type": "module",
  "bin": {
    "ensolid": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haike0513/ensolid.git",
    "directory": "packages/cli"
  },
  "homepage": "https://ensolid.dev",
  "bugs": {
    "url": "https://github.com/haike0513/ensolid/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean --dts",
    "dev": "tsup src/index.ts --format esm --watch --clean",
    "lint": "eslint src",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "solidjs",
    "solid",
    "shadcn",
    "shadcn-ui",
    "cli",
    "ui",
    "components",
    "tailwindcss",
    "radix"
  ],
  "author": "haike0513",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
