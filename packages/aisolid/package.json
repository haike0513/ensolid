{
  "name": "@ensolid/aisolid",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./useChat": {
      "import": "./dist/hooks/useChat.js",
      "types": "./dist/hooks/useChat.d.ts"
    },
    "./useCompletion": {
      "import": "./dist/hooks/useCompletion.js",
      "types": "./dist/hooks/useCompletion.d.ts"
    },
    "./useAssistant": {
      "import": "./dist/hooks/useAssistant.js",
      "types": "./dist/hooks/useAssistant.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc --noEmit && vite build",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "solid-js": "^1.9.9"
  },
  "devDependencies": {
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "solid-js": "^1.9.9",
    "ai": "^6.0.0"
  }
}
