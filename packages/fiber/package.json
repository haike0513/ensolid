{
  "name": "@ensolid/fiber",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./Canvas": {
      "import": "./dist/web/Canvas.js",
      "types": "./dist/web/Canvas.d.ts"
    },
    "./renderer": {
      "import": "./dist/renderer.js",
      "types": "./dist/renderer.d.ts"
    },
    "./loader": {
      "import": "./dist/web/loader.js",
      "types": "./dist/web/loader.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@types/three": "^0.170.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-solid": "^2.11.8"
  },
  "peerDependencies": {
    "solid-js": "^1.9.9"
  }
}
