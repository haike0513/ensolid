{
  "name": "ensolid",
  "private": "false",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm -r --filter './packages/*' build && tsc -b && vite build",
    "preview": "vite preview",
    "build:baseui": "pnpm --filter @ensolid/baseui build",
    "dev:baseui": "pnpm --filter @ensolid/baseui dev",
    "build:solidflow": "pnpm --filter @ensolid/solidflow build",
    "dev:solidflow": "pnpm --filter @ensolid/solidflow dev",
    "build:radix": "pnpm --filter @ensolid/radix build",
    "dev:radix": "pnpm --filter @ensolid/radix dev",
    "build:aisolid": "pnpm --filter @ensolid/aisolid build",
    "dev:aisolid": "pnpm --filter @ensolid/aisolid dev",
    "build:remend": "pnpm --filter @ensolid/remend build",
    "dev:remend": "pnpm --filter @ensolid/remend dev",
    "build:streamdown": "pnpm --filter @ensolid/streamdown build",
    "dev:streamdown": "pnpm --filter @ensolid/streamdown dev",
    "publish": "pnpm -r --filter './packages/*' publish",
    "publish:baseui": "pnpm --filter @ensolid/baseui publish",
    "publish:remend": "pnpm --filter @ensolid/remend publish",
    "publish:radix": "pnpm --filter @ensolid/radix publish",
    "publish:streamdown": "pnpm --filter @ensolid/streamdown publish",
    "publish:solidflow": "pnpm --filter @ensolid/solidflow publish",
    "publish:aisolid": "pnpm --filter @ensolid/aisolid publish"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
    "@ai-sdk/devtools": "^0.0.2",
    "@ai-sdk/openai": "^3.0.1",
    "@ai-sdk/openai-compatible": "^2.0.1",
    "@ensolid/baseui": "workspace:*",
    "@ensolid/fiber": "workspace:*",
    "@ensolid/radix": "workspace:*",
    "@ensolid/remend": "workspace:*",
    "@ensolid/solidflow": "workspace:*",
    "@ensolid/streamdown": "workspace:*",
    "@ensolid/visx": "workspace:*",
    "@solidjs/router": "^0.15.4",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "@xyflow/system": "^0.0.74",
    "ai": "^6.0.3",
    "solid-js": "^1.9.9",
    "tailwindcss": "^4.1.18",
    "throttleit": "^2.1.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "clsx": "^2.1.1",
    "tsup": "^8.5.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-solid": "^2.11.8"
  }
}
